{
  "name": "simple-qr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "qrcode.vue": "^1.7.0",
    "sass-loader": "^9.0.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/attribute-hyphenation": 1,
      "vue/html-closing-bracket-newline": 2,
      "vue/html-closing-bracket-spacing": 2,
      "vue/html-end-tags": 2,
      "vue/html-indent": 1,
      "vue/html-quotes": 2,
      "vue/html-self-closing": 1,
      "vue/multiline-html-element-content-newline": 1,
      "vue/mustache-interpolation-spacing": 1,
      "vue/name-property-casing": 1,
      "vue/no-multi-spaces": 1,
      "vue/no-spaces-around-equal-signs-in-attribute": 1,
      "vue/no-template-shadow": 2,
      "vue/prop-name-casing": 1,
      "vue/require-default-prop": 1,
      "vue/require-prop-types": 1,
      "vue/v-bind-style": 1,
      "vue/v-on-style": 1,
      "vue/attributes-order": 1,
      "vue/no-confusing-v-for-v-if": 1,
      "vue/no-v-html": 2,
      "vue/order-in-components": 1,
      "vue/this-in-template": 1,
      "vue/array-bracket-spacing": 1,
      "vue/arrow-spacing": 1,
      "vue/block-spacing": 1,
      "vue/brace-style": 1,
      "vue/camelcase": 1,
      "vue/component-name-in-template-casing": 1,
      "vue/eqeqeq": 1,
      "vue/key-spacing": 1,
      "vue/match-component-file-name": 2,
      "vue/no-boolean-default": 1,
      "vue/object-curly-spacing": 1,
      "vue/require-direct-export": 1,
      "vue/script-indent": 1,
      "vue/space-infix-ops": 1,
      "vue/space-unary-ops": 1,
      "vue/v-on-function-call": 2
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
